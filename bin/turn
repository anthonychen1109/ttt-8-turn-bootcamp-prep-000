#!/usr/bin/env ruby
require_relative '../lib/turn'
board = [" "," "," "," "," "," "," "," "," "]

puts "Welcome to Tic Tac Toe!"
display_board(board)

puts "Pick a number between 1 and 9"
input = gets.strip
index = input_to_index(input)
if valid_move?(board,index)
  move(board, index)
end
display_board(board)
